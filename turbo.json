{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "cache": false,
            "dependsOn": ["^build"]
        },
        "clean": {
            "dependsOn": ["^clean"]
        },
        "release": {
            "dependsOn": ["^release"]
        },
        "test": {
            "dependsOn": ["^test"]
        }
    }
}
